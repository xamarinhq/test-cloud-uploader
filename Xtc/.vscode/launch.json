{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/Xtc.Cli/bin/Debug/netcoreapp1.0/osx.10.11-x64/xtc.dll",
            "args": [ "test", "/Users/tomaskmi/appium.app.ipa", "APIKEY", "--user", "test@test.com", "--devices", "DEVICES" ],
            "cwd": "${workspaceRoot}/src/Xtc.Cli",
            "externalConsole": false,
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}